import Connector4Pin from "connector.ato"
import ISOCAN from "generics.ato"
import Power from "generics.ato"

component JSTGH4Pin from Connector4Pin:
    isocan = new ISOCAN
    isocan.CANH ~ p2
    isocan.CANL ~ p3

    power = new Power
    power.vcc ~ p1
    power.gnd ~ p4

    footprint = "lib:CONN-SMD_4P-P1.25_SM04B-GHS-TB-LF-SN"
