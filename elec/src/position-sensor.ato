import SPI from "generics/interfaces.ato"
import Power from "generics/interfaces.ato"
import MA732 from "ma732/elec/src/ma732.ato"

module PositionSensor:
    power = new Power
    spi = new SPI

    # Components
    ma732 = new MA732

    # Internal Connections
    power ~ ma732.power
    spi ~ ma732.spi
